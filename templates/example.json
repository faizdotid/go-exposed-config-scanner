{
    "id": "example",
    "name": "Example",
    "output": "example.txt",
    "request": {
        "method": "GET",
        "headers": {
            "Accept": "text/plain"
        },
        "timeout": 7
    },
    "match": {
        "from": "body",
        "type": "regex",
        "value": "^[A-Z_]+=[^\\n]+"
    },
    "paths": [
        "/example"
    ]
}